{
  "name": "ETU-forms",
  "version": "0.0.1",
  "description": "API for ETU-forms",
  "author": "Andy",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"src/**/*.ts\" --quiet",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --quiet",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "c8 jest",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:ci": "jest --runInBand --detectOpenHandles --forceExit --noStackTrace --silent=true",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d config/datasource.ts",
    "migration:generate": "npm run typeorm:cli -- migration:generate ./src/migrations/$name",
    "migration:create": "typeorm migration:create ./src/migrations/$name",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:revert": "npm run typeorm:cli -- migration:revert",
    "fixtures": "node truncateTables.js && fixtures load ./config/fixtures/*.yml -d ./dist/config/datasource.js --require=ts-node/register --require=tsconfig-paths/register"
  },
  "dependencies": {
    "@fosfad/openapi-typescript-definitions": "^0.0.28",
    "@ivankrtv/openapidoc": "^0.3.2",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/cli": "^8.2.5",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^9.0.0",
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.1.1",
    "@types/node": "^16.0.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "create-hmac": "^1.1.7",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.1",
    "easy-yandex-s3": "^1.1.6",
    "exponential-backoff": "^3.1.1",
    "ioredis": "^5.4.1",
    "jwt-decode": "^3.1.2",
    "mailgun.js": "^5.0.2",
    "moment": "^2.29.3",
    "pg": "^8.7.3",
    "pgtools": "^0.3.2",
    "pino": "^9.5.0",
    "redoc-express": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.3.0",
    "timekeeper": "^2.3.1",
    "typeorm": "^0.3.20",
    "typeorm-fixtures-cli": "^4.0.0",
    "uniteller-helper": "^0.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.3",
    "@redocly/cli": "^1.12.2",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.0",
    "@types/node-fetch": "^2.6.11",
    "@types/sinon": "^17.0.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "globals": "^15.9.0",
    "jest": "^27.5.1",
    "object-oriented-tests-jest": "^0.4.3",
    "prettier": "^2.3.2",
    "sinon": "^19.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "testTimeout": 300000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "moduleNameMapper": {
      "^@domain/(.*)$": "<rootDir>/src/domain/$1",
      "^@infrastructure/(.*)$": "<rootDir>/src/infrastructure/$1",
      "^@applications/(.*)$": "<rootDir>/src/application/$1",
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
